<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.oganbelema.agromall">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".AgroMallApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/AppTheme">
        <activity android:name=".farmer.ui.EditFarmerDataActivity"
            android:parentActivityName=".farmer.ui.FarmerListActivity"
            android:label="@string/edit_farmer"/>
        <activity android:name=".farmer.ui.FarmerListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service android:name=".farmer.service.SyncFarmerDataService"/>
    </application>

</manifest>